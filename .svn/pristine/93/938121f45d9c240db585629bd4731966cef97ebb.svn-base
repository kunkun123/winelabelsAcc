<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gecko.framework.param.repository.dao.InventorycontrolDao">
	<resultMap id="Inventorycontrol" type="org.gecko.framework.param.entity.Inventorycontrol">
		<result property="id" column="ID" jdbcType="VARCHAR"/>
		<result property="credit" column="credit" jdbcType="VARCHAR" />
		<result property="circulating" column="circulating" jdbcType="VARCHAR" />
		<result property="uncirculation" column="uncirculation" jdbcType="VARCHAR" />
		<result property="ent" column="ent" jdbcType="VARCHAR" />
		<result property="unused" column="unused" jdbcType="VARCHAR" />
		<result property="finish" column="finish" jdbcType="VARCHAR" />
		<result property="total" column="total" jdbcType="VARCHAR" />
	</resultMap>
	<select id="count" resultType="int" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
		select count(*) from INVENTORYCONTROL t where 1=1
	</select>
	
	<select id="get" resultMap="Inventorycontrol" parameterType="String">
		select  Inventorycontrol.* from INVENTORYCONTROL Inventorycontrol where Inventorycontrol.ID = #{id}
	</select>

	<insert id="save" parameterType="org.gecko.framework.param.entity.Inventorycontrol">	
 		insert into INVENTORYCONTROL(ID,credit,circulating,uncirculation,ent,unused,finish,total)
 		VALUES(#{id},#{credit,jdbcType=VARCHAR},#{circulating,jdbcType=VARCHAR},#{uncirculation,jdbcType=VARCHAR},#{ent,jdbcType=VARCHAR},#{unused,jdbcType=VARCHAR},#{finish,jdbcType=VARCHAR},#{total,jdbcType=VARCHAR})
	</insert>
	
	<insert id="batchSave" parameterType="java.util.List">	
 		insert into INVENTORYCONTROL(ID,credit,circulating,uncirculation,ent,unused,finish,total)
		values
		<foreach item="o" collection="list"  separator="," >
 		(#{o.id},#{o.credit,jdbcType=VARCHAR},#{o.circulating,jdbcType=VARCHAR},#{o.uncirculation,jdbcType=VARCHAR},#{o.ent,jdbcType=VARCHAR},#{o.unused,jdbcType=VARCHAR},#{o.finish,jdbcType=VARCHAR},#{o.total,jdbcType=VARCHAR})
		</foreach>
	</insert>
	
	<update id="update" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
    		update INVENTORYCONTROL set credit=#{credit}, circulating=#{circulating}, uncirculation=#{uncirculation}, ent=#{ent}, unused=#{unused}, finish=#{finish}, total=#{total} where ID=#{id}
	</update>
	<delete id="delete" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
		delete from INVENTORYCONTROL where ID=#{id}
	</delete>
	
	<delete id="deleteById" parameterType="String">
		delete from INVENTORYCONTROL where ID=#{id}
	</delete>
	
	<delete id="batchDelete">
		delete from INVENTORYCONTROL where ID in
		<foreach item="ids" collection="array" open="(" separator="," close=")" >#{ids}</foreach>
	</delete>
	
	<delete id="batchDeleteBy">
        delete from INVENTORYCONTROL where         <if test="credit!=null">and credit = #{credit}</if><if test="circulating!=null">and circulating = #{circulating}</if><if test="uncirculation!=null">and uncirculation = #{uncirculation}</if><if test="ent!=null">and ent = #{ent}</if><if test="unused!=null">and unused = #{unused}</if><if test="finish!=null">and finish = #{finish}</if><if test="total!=null">and total = #{total}</if>    </delete>

    <select id="find" resultMap="Inventorycontrol" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
        select Inventorycontrol.* from INVENTORYCONTROL Inventorycontrol where 1=1 <if test="id!=null">and ID=#{id}</if>
        <if test="credit!=null"> and credit = #{credit} </if><if test="circulating!=null"> and circulating = #{circulating} </if><if test="uncirculation!=null"> and uncirculation = #{uncirculation} </if><if test="ent!=null"> and ent = #{ent} </if><if test="unused!=null"> and unused = #{unused} </if><if test="finish!=null"> and finish = #{finish} </if><if test="total!=null"> and total = #{total} </if>    </select>

	<select id="findByPage" resultMap="Inventorycontrol" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
		select Inventorycontrol.* from INVENTORYCONTROL Inventorycontrol where 1=1 <if test="param.id!=null">and ID=#{id}</if>
		<if test="param.credit!=null"> and credit = #{param.credit} </if><if test="param.circulating!=null"> and circulating = #{param.circulating} </if><if test="param.uncirculation!=null"> and uncirculation = #{param.uncirculation} </if><if test="param.ent!=null"> and ent = #{param.ent} </if><if test="param.unused!=null"> and unused = #{param.unused} </if><if test="param.finish!=null"> and finish = #{param.finish} </if><if test="param.total!=null"> and total = #{param.total} </if>	</select>

    <select id="likeByPage" resultMap="Inventorycontrol" parameterType="org.gecko.framework.param.entity.Inventorycontrol">
        select Inventorycontrol.* from INVENTORYCONTROL Inventorycontrol where 1=1
        <if test="param.credit!=null"> and credit like #{param.credit}</if><if test="param.circulating!=null"> and circulating like #{param.circulating}</if><if test="param.uncirculation!=null"> and uncirculation like #{param.uncirculation}</if><if test="param.ent!=null"> and ent like #{param.ent}</if><if test="param.unused!=null"> and unused like #{param.unused}</if><if test="param.finish!=null"> and finish like #{param.finish}</if><if test="param.total!=null"> and total like #{param.total}</if>    </select>

    <update id="updateField" parameterType="String">
        UPDATE INVENTORYCONTROL SET ${field} = #{value} WHERE ID = #{pk}
    </update>
</mapper>