    <select id="likeByPage" resultMap="Needtaxcontrol" parameterType="org.gecko.framework.param.entity.Needtaxcontrol">
        select Needtaxcontrol.* from NEEDTAXCONTROL Needtaxcontrol where 1=1
        <if test="param.other!=null"> and OTHER like #{param.other}</if><if test="param.deadline!=null"> and DEADLINE like #{param.deadline}</if><if test="param.beer!=null"> and BEER like #{param.beer}</if><if test="param.yellow!=null"> and YELLOW like #{param.yellow}</if><if test="param.white!=null"> and WHITE like #{param.white}</if><if test="param.needtax!=null"> and NEEDTAX like #{param.needtax}</if><if test="param.ent!=null"> and ENT like #{param.ent}</if><if test="param.credit!=null"> and CREDIT_ like #{param.credit}</if>    </select>

    <update id="updateField" parameterType="String">
