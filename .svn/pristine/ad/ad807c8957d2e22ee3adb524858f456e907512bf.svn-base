drop table if exists CP_COMPLAINTINFO;
create table CP_COMPLAINTINFO(
	ID NUMBER(22) not null ,

	TEMP2 VARCHAR2(80),

	TEMP1 VARCHAR2(80),

	IP VARCHAR2(20),

	COMPLAINANTTYPE VARCHAR2(2),

	COMPLAINANTLICENSE VARCHAR2(60),

	ADDRESS VARCHAR2(80),

	TEL VARCHAR2(20),

	ZIPCODE VARCHAR2(10),

	COMPLAINANTNAME VARCHAR2(20),

	EMAIL VARCHAR2(60),

	LICENSETYPE VARCHAR2(2)
);
