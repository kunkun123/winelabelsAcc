.modal-header{
	height:30px;
	padding:5px;
	font-size: 14px !important;
	border-top-left-radius:5px !important;
	border-top-right-radius:5px !important;
}
.role-box{
	width:100%;
	height:78px;
}
.role-select{
	height:36px;
}
.role-input{
	width:100%;
	height:40px;
	line-height: 20px;
	border:solid 1px #CCCCCC;
	padding: 4px 4px 0px 4px;
	margin-top:2px;
	
}
.search-choice{
  width:60px;
  height:25px;
  position: relative;
  margin: 3px 5px 3px 0;
  padding: 3px 0px 3px 5px;
  border: 1px solid #aaa;
  max-width: 100%;
  border-radius: 3px;
  background-color: #F2F2F2;
  
  box-shadow: 0 0 2px white inset, 0 1px 0 rgba(0, 0, 0, 0.05);
  color: #333;
  line-height:18px;
  cursor: default;
  font-size:12px;
}
.search-choice span{
	color:#515051;
}
.role-tree{
	width:100%;
	height:330px;
	border:solid 1px #CCCCCC;
	margin-top:5px;
	overflow-y: scroll;
}
.modal-footer{
	/*height:34px;
	width:368px;*/
	background:#FFFFFF;
	text-align:center;
	padding-top:0px;
	padding-bottom:18px !important;
	border-top:none !important;
}
